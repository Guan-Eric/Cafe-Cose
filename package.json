{
  "name": "cafe-cose",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "prebuild": "expo prebuild",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "web": "expo start --web",
    "dev:build:ios": "eas build --profile development --platform ios",
    "dev:build:android": "eas build --profile development --platform android",
    "prod:build:ios": "eas build --profile production --platform ios --auto-submit",
    "prod:build:android": "eas build --profile production --platform android --auto-submit"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "8.3.0",
    "@react-native-picker/picker": "2.11.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "expo": "53.0.11",
    "expo-camera": "~16.1.8",
    "expo-constants": "~17.1.5",
    "expo-dev-client": "~5.2.0",
    "expo-device": "~7.1.4",
    "expo-image-picker": "~16.1.4",
    "expo-linking": "~7.1.5",
    "expo-notifications": "~0.31.3",
    "expo-router": "~5.1.0",
    "expo-splash-screen": "~0.30.9",
    "expo-status-bar": "~2.2.3",
    "expo-updates": "~0.28.14",
    "firebase": "^11.6.0",
    "fix": "^0.0.6",
    "nativewind": "latest",
    "react": "19.0.0",
    "react-content-loader": "^7.0.2",
    "react-native": "0.79.3",
    "react-native-qrcode-svg": "^6.3.15",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~19.0.10",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "^8.57.0",
    "eslint-config-universe": "^12.0.1",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "react-native-svg-transformer": "^1.5.0",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.8.3"
  },
  "eslintConfig": {
    "extends": "universe/native",
    "root": true
  },
  "private": true
}
